    "description": "Composer plugin to convert a composer.yml to json or vice-versa.",
    "keywords": ["composer", "yaml"],
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Haso≈à",
            "email": "martin.hason@gmail.com"
        },
        {
            "name": "Webuni s.r.o.",
            "homepage": "https://www.webuni.cz"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "symfony/yaml": "^2.8 || ^3.0",
        "composer-plugin-api": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.3",
